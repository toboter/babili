<% title 'People', false %>

<% content_for :breadcrumbs do %>
  <%= content_tag :li, yield(:title), class: 'active' %>
<% end %>

<div class="row list-group">
  <% @people.each do |person| %>
    <div class="col-md-12" style="margin-bottom:20px;">
      <%= render person %>
    </div>
  <% end %>
</div>