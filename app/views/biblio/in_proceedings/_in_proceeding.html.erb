<tr>
  <td width="5%">
    <%= content_tag :span, fa_icon(in_proceeding.icon), title: 'In proceeding' %>
  </td>
  <td>
    <%= render 'biblio/shared/actions', entry: in_proceeding, referencation: local_assigns[:referencation] %>
    <%= content_tag :span, link_to(in_proceeding.citation, in_proceeding), class: 'text-strong' %><br>
    <%= content_tag :span, in_proceeding.bibliographic(current_user.try(:person).try(:csl), 'en-US'), class: 'text-small' %>
  </td>
</tr>