<tr id="biblio-entry-<%= techreport.id %>">
  <td width="5%">
    <%= content_tag :span, fa_icon(techreport.icon), title: 'Techreport' %>
  </td>
  <td width="95%">
    <%= render 'biblio/shared/actions', entry: techreport, referencation: local_assigns[:referencation], actions: local_assigns[:actions] %>
    <%= content_tag :span, link_to(techreport.citation, techreport), class: 'text-strong' %><br>
    <%= content_tag :span, techreport.bibliographic(current_user.try(:person).try(:csl), 'en-US'), class: 'text-small' %>
  </td>
</tr>